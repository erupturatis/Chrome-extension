{
  "manifest_version": 3,
  "name": "PageEncryption",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"],
      "web_accessible_resources": ["popup.js", "Content.js"]
    }
  ],

  "action": {
    "browser_style": true,
    "default_icon": {
      "16": "icon16.png"
    },
    "default_title": "PageEncryption",
    "default_popup": "popup.html"
  }
}
